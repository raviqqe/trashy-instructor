	"devDependencies": {
		"@types/enzyme": "^3.1.11",
		"@types/jest": "^23.1.4",
		"@types/lodash": "^4.14.110",
		"@types/react": "^16.4.6",
		"@types/react-dom": "^16.0.6",
		"@types/react-redux": "^6.0.3",
		"electron": "^2.0.2",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"jest": "^23.3.0",
		"lodash": "^4.17.10",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-icons": "^2.2.7",
		"react-redux": "^5.0.7",
		"redux": "^4.0.0",
		"styled-components": "^3.3.3",
		"ts-jest": "^23.0.0",
		"ts-loader": "^4.4.2",
		"tslint": "^5.10.0",
		"tslint-react": "^3.6.0",
		"typescript": "^2.8.3",
		"typescript-fsa": "^3.0.0-beta-2",
		"typescript-fsa-reducers": "^0.4.5",
		"webpack": "^4.15.0",
		"webpack-cli": "^3.0.8"
	},
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
